{
	"useTabs": true,
	"printWidth": 80,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"svelteSortOrder": "options-scripts-styles-markup",
	"svelteStrictMode": false,
	"svelteBracketNewLine": true
}
